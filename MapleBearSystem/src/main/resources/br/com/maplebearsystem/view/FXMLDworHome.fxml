<BorderPane fx:id="rootPane" minHeight="700.0" minWidth="980.0" style="-fx-background-color: white;" stylesheets="@FXMLStylesheet.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.com.maplebearsystem.view.controller.FXMLDworHomeController">
