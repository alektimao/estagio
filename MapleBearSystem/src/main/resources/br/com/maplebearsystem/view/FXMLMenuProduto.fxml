      				<JFXButton id="NavigationMenuButton" fx:id="btnCadastroProduto" maxWidth="1.7976931348623157E308" onAction="#actCadastroProduto" text="Cadastro" textAlignment="CENTER" wrapText="true" HBox.hgrow="ALWAYS">
