      					</styleClass>
      				</JFXButton>
      				<JFXButton id="NavigationMenuButton" fx:id="btnReceberProduto" maxWidth="1.7976931348623157E308" onAction="#actReceberProduto" text="Receber Produtos" HBox.hgrow="ALWAYS">
