<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXPasswordField?>
<?import com.jfoenix.controls.JFXTextArea?>
<?import com.jfoenix.controls.JFXTextField?>
<?import com.jfoenix.controls.JFXToggleButton?>
<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox id="pnEdit" fx:id="pnEdit" alignment="TOP_CENTER" minWidth="338.0" prefHeight="200.0" prefWidth="338.0" styleClass="parent" stylesheets="@../FXMLStylesheet.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox alignment="CENTER" spacing="14.0">
         <children>
            <JFXTextField id="tfieldName" fx:id="tfieldName" editable="false" labelFloat="true" prefHeight="26.0" prefWidth="212.0" promptText="Quem" styleClass="jfxtextfieldform" HBox.hgrow="ALWAYS" />
            <JFXButton fx:id="btnSPNew" buttonType="RAISED" onAction="#actSPNew" prefWidth="150.0" ripplerFill="#282a2f" text="Selecionar">
               <effect>
                  <DropShadow blurType="GAUSSIAN" height="30.0" radius="14.5" width="30.0" />
               </effect>
               <styleClass>
                  <String fx:value="JFXButtonDefault" />
                  <String fx:value="JFXButtonAccept" />
                  <String fx:value="JFXButtonCompactII" />
               </styleClass>
            </JFXButton>
         </children>
         <VBox.margin>
            <Insets bottom="14.0" left="14.0" right="14.0" top="14.0" />
         </VBox.margin>
         <styleClass>
            <String fx:value="jfxtextfieldform" />
            <String fx:value="formMaxWidth" />
         </styleClass>
      </HBox>
      <HBox spacing="14.0">
         <children>
            <JFXTextField id="tfieldFuncao" fx:id="tfieldFuncao" labelFloat="true" promptText="Função" styleClass="jfxtextfieldform" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </JFXTextField>
            <JFXTextField id="tfieldMatricula" fx:id="tfieldMatricula" labelFloat="true" prefHeight="26.0" prefWidth="94.0" promptText="Matricula" styleClass="jfxtextfieldform">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </JFXTextField>
         </children>
         <VBox.margin>
            <Insets bottom="14.0" left="14.0" right="14.0" top="14.0" />
         </VBox.margin>
         <styleClass>
            <String fx:value="jfxtextfieldform" />
            <String fx:value="formMaxWidth" />
         </styleClass>
      </HBox>
      <HBox alignment="CENTER" spacing="14.0">
         <children>
            <JFXPasswordField id="pwfieldSenha" fx:id="pwfieldSenha" promptText="Senha" HBox.hgrow="ALWAYS" />
         </children>
         <VBox.margin>
            <Insets bottom="14.0" left="14.0" right="14.0" top="14.0" />
         </VBox.margin>
         <styleClass>
            <String fx:value="jfxtextfieldform" />
            <String fx:value="formMaxWidth" />
         </styleClass>
      </HBox>
      <HBox alignment="CENTER" spacing="14.0">
         <VBox.margin>
            <Insets bottom="14.0" left="14.0" right="14.0" top="14.0" />
         </VBox.margin>
         <styleClass>
            <String fx:value="jfxtextfieldform" />
            <String fx:value="formMaxWidth" />
         </styleClass>
         <children>
            <JFXTextArea id="tareaObs" fx:id="tareaObs" labelFloat="true" maxHeight="400.0" prefHeight="35.0" prefWidth="323.0" promptText="Observações">
               <styleClass>
                  <String fx:value="jfxtextfieldform" />
                  <String fx:value="formMaxWidth" />
               </styleClass>
            </JFXTextArea>
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="40.0" prefWidth="200.0" spacing="14.0">
         <VBox.margin>
            <Insets bottom="14.0" left="14.0" right="14.0" top="14.0" />
         </VBox.margin>
         <children>
            <VBox alignment="CENTER" maxWidth="110.0">
               <children>
                  <Label alignment="CENTER" contentDisplay="CENTER" minWidth="110.0" text="Ativar" textAlignment="CENTER" wrapText="true" />
