                        <Font name="System Bold" size="14.0" />
                     </font>
                     <graphic>
                        <FontAwesomeIconView fill="WHITE" glyphName="UNDO" size="15" stroke="#755d5d" strokeType="OUTSIDE" strokeWidth="1.5" />
                     </graphic>
