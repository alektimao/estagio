                        </ContextMenu>
                     </contextMenu>
                  </JFXComboBox>
                  <Label fx:id="lbl_abertura" alignment="BOTTOM_CENTER" prefHeight="36.0" text="DT.Inicio">
                     <font>
                        <Font size="14.0" />
                     </font>
                     <tooltip>
                        <Tooltip fx:id="ttp_lblClienteT1" text="Data de abertura da parcela." />
                     </tooltip>
                  </Label>
                  <JFXDatePicker fx:id="dp_aberturaIni" prefHeight="36.0" prefWidth="150.0" />
                  <Label fx:id="lbl_aberturaFinal" alignment="BOTTOM_CENTER" prefHeight="36.0" text="DT.Fim">
                     <font>
                        <Font size="14.0" />
                     </font>
                     <tooltip>
                        <Tooltip fx:id="ttp_lblVencimentoT1" text="Data de vencimento da parcela." />
                     </tooltip>
                  </Label>
                  <JFXDatePicker fx:id="dp_aberturaFim" prefHeight="36.0" prefWidth="131.0" />
